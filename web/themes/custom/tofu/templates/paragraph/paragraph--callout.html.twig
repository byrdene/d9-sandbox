
{%
  set classes = [
  'component',
  'component--type-' ~ paragraph.bundle|clean_class,
  'component-paragraph',
  'container',
  'bg__primary',
  custom_class,
]
%}

<section{{ attributes.addClass(classes) }}>
  <div class="callout-wrapper container medium-width padded-row">


    <div class="callout-content container padded-column">

      <div class="text-wrapper h2 text__centered">
        {{ content.field_text }}
      </div>


    </div>
  </div>
</section>
